@{
    ViewBag.Title = "addNewsFeedData";
}


<h2  style="font-family:'Times New Roman';font-size:24px;color:blue">Add News in English as Well in Urdu:</h2>

<form action="/Admin/SaveNewsFeedData/@Model.Id" method="post">
    <label style="font-family:'Times New Roman';font-size:20px">Enter English News: </label><br /><textarea  name="data1" style="background-color:white" rows="5" cols="30" required="" class="form-control">@Model.EnglishNews</textarea><br /><br />
     <label style="font-family:'Times New Roman';font-size:20px">Enter Urdu News: </label><br /><textarea  name="data2" style="background-color:white" rows="5" cols="30" required="" class="form-control">@Model.UrduNews</textarea> <br />

    <input type="submit" value="Submit" style="margin-top:20px;width:100px;height:40px;margin-left:500px;color:blue;font-synthesis:weight;border:double;border-color:red;box-shadow:inset;text-shadow:40px 40px;border-collapse:collapse;font-size:20px"/>
</form>