@{
    ViewBag.Title = "addContactUsData";
}

<h2 style="font-family:'Times New Roman';font-size:24px;color:blue">Add Contact Us Data</h2>
<form action="/Admin/SaveContactUsData/@Model.Id" method="post">
            <label style="font-family:'Times New Roman';font-size:20px">Enter GOVERNOR'S SECRETARIAT U.A.N Number:<br /> </label> <textarea  name="data6" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.UanNo</textarea><br /><br />
        <label style="font-family:'Times New Roman';font-size:20px">Enter GOVERNOR'S SECRETARIAT Emergency Number:<br /> </label> <textarea  name="data7" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.EmergencyNo</textarea><br /><br />
        <label style="font-family:'Times New Roman';font-size:20px">Enter GOVERNOR'S SECRETARIAT Phone Number:<br /> </label> <textarea  name="data8" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.PhoneNo</textarea><br /><br />
        <label style="font-family:'Times New Roman';font-size:20px">Enter GOVERNOR'S SECRETARIAT Fax Number:<br /> </label> <textarea  name="data9" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.FaxNo</textarea><br /><br />

    <label style="font-family:'Times New Roman';font-size:20px">Enter Cplc South Zone Phone Numbers:<br /> </label> <textarea  name="data1" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.CplcSouth</textarea><br /><br />
    <label style="font-family:'Times New Roman';font-size:20px">Enter Cplc West Zone Phone Numbers:<br /> </label> <textarea  name="data2" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.CplcWest</textarea><br /><br />
        <label style="font-family:'Times New Roman';font-size:20px">Enter East South Zone Phone Numbers:<br /> </label> <textarea  name="data3" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.CplcEast</textarea><br /><br />
    <label style="font-family:'Times New Roman';font-size:20px">Enter Cplc Centeral Zone Phone Numbers:<br /> </label> <textarea  name="data4" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.CplcCenteralv</textarea><br /><br />
        <label style="font-family:'Times New Roman';font-size:20px">Enter Cplc Malir Zone Phone Numbers:<br /> </label> <textarea  name="data5" style="background-color:white" rows="1" cols="30" required="" class="form-control">@Model.CplcMalir</textarea><br /><br />

    
    <input type="submit" value="Submit" style="width:100px;height:40px;margin-left:500px;color:blue;font-synthesis:weight;border:double;border-color:red;box-shadow:inset;text-shadow:40px 40px;border-collapse:collapse;font-size:20px"/>
</form>
