@{
    ViewBag.Title = "Linkks";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="background-color:burlywood;margin-bottom:100px;padding-bottom:100px">
<h2 style="background-color:blue;font-family:'Times New Roman';font-size:20px;color:white;margin-top:80px;margin-left:70px;margin-right:80px;margin-top:-30px;text-align:center"><b><u>Usefull Links</u></b></h2>
<p style="background-color:red;margin-top:20px;margin-left:70px;margin-right:80px;height:22px"></p>

<div  style="background-color:burlywood;margin-top:10px;margin-left:80px;margin-right:90px;margin-bottom:100px;">
<table>
 <tr>  <td style="font-size:25px;color:blue;font-family:'Times New Roman';margin-left:50px"><b>Name with Designation</b></td>
     <td>         </td>
    <td style="font-size:25px;color:blue;font-family:'Times New Roman';padding-left:300px"><b>Link </b></td></tr>
    
    @foreach (var s in Model)
    {
        <tr>
             <td style="font-size:17px;padding-left:30px;font-family:'Times New Roman'"> @s.Name </td>
            <td>    </td>
            <td style="font-size:17px;padding-left:30px;font-family:'Times New Roman';padding-left:300px"><a href="@s.Linkk" target="_blank"> @s.Linkk </a></td>
         
        </tr>
    }
   
</table>
    </div>
    </div>