@{
    ViewBag.Title = "AboutUs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div style="font-family:'Times New Roman';font-size:15px;margin-top:0px;margin-left:150px;margin-right:30px;text-align:justify;background-color:burlywood">
    
        <h2 style="color:blue;margin-top:20px;margin-left:0px"><u>About Us:</u></h2>
        <table>
        @foreach(var s in Model)
        {
        <tr>
          <td><p  style="font-size:15px;font-family:'Times New Roman';text-align:justify">@s.Info</p></td>  
        </tr>
        }
    </table>
  
</div>
